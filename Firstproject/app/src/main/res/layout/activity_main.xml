    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.firstproject.MainActivity"
    android:id="@+id/mainLayout">


    <include
        layout="@layout/token_imagebutton"
        tools:layout_editor_marginStart="180dp"
        tools:layout_editor_margintop="240dp" />

    <TextView
        android:id="@+id/scoreText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="none"
        android:text= "uninitialized"
        android:onClick="tokenTap"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>
